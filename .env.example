# Environment
NODE_ENV=development

# Database (required)
# PostgreSQL connection string - local or hosted (e.g., Vercel Postgres, Neon, Supabase)
POSTGRES_URL=

# ChatJimmy (required for chat)
# Get your bot ID from: https://chatjimmy.ai
CHATJIMMY_BOT_ID=default

# v0 API (optional - for chat history/demo URLs if using v0)
V0_API_KEY=

# Optional: Custom v0 API URL (leave empty for default)
V0_API_URL=
