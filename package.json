    "check": "biome check .",
    "check:fix": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "biome check --no-errors-on-unmatched"
    ],
    "*.{json,md,css}": [
      "biome format --no-errors-on-unmatched"
    ]
  },
